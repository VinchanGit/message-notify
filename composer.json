{
    "name": "vinchan/message-notify",
    "description": "MIT",
    "license": "MIT",
    "authors": [
        {
            "name": "Vinchan",
            "email": "ademo@vip.qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MessageNotify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MessageNotifyTest\\": "test/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "hyperf/guzzle": "^1.1|^2.1|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.4",
        "hyperf/di": "^2.2",
        "hyperf/utils": "^3.0",
        "hyperf/config": "*",
        "hyperf/ide-helper": "v2.2.*"
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "cs-fix": "./vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "MessageNotify\\ConfigProvider"
        }
    }
}
